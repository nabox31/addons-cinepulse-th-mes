{
  "manifest_version": 3,
  "name": "CinePulse Themes",
  "version": "1.0",
  "description": "Change le th√®me de CinePulse",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://cinepulse.cc/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://cinepulse.cc/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
